<%- include('../partials/header') %>

<h2><%= artist.firstName %> <%= artist.lastName %></h2>

<p>Styles: <%= artist.artistProf.styles.join(', ') %></p>

<% posts.forEach(post => { %>
    <img src="data:img/png;base64,<%= post.content[0].image.data.toString('base64') %>"<br><br>
    <h3><%= post.caption %></h3>
    <p><%= post.tags.join(', ') %></p>
<% }); %>


<%- include('../partials/footer') %>